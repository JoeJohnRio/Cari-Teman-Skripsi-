<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/space_really_small">
        <TextView
            android:id="@+id/tv_nama_skill_hobi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="Joel Johanes Cirio"
            android:textColor="@color/black_effective"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
        <CheckBox
            android:id="@+id/cb_check_skill"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/grey_mid"
            app:layout_constraintBottom_toBottomOf="@id/cb_check_teman"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>